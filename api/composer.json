{
    "name": "marcinbondaruk/php-fooder",
    "description": "personal meals helper in php",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/",
            "Database\\Migrations\\": "database/migrations"
        }
    },
    "authors": [
        {
            "name": "Marcin Bondaruk",
            "email": "marcin.bondaruk@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "symfony/http-foundation": "^7.1",
        "symfony/http-kernel": "^7.1",
        "symfony/routing": "^7.1",
        "symfony/event-dispatcher": "^7.1",
        "symfony/dependency-injection": "^7.1",
        "doctrine/migrations": "3.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "11.3.1"
    },
    "scripts": {
        "new-migration": "./vendor/bin/doctrine-migrations generate --configuration ./config/migrations.php"
    }
}
